class Program
{
    static void Main()
    {
        double c = 0;
        double max = 0;
        double min = 10;
        int count = 0;
        String StringNumber = Console.ReadLine();
        Char[] CharNumber = StringNumber.ToCharArray();
        for (int i = 0; i < CharNumber.Length; i++)
        {
            c = Convert.ToInt32(StringNumber[i] - '0');
            if (c > max)
            {
                max = c;
            }
            if (c < min)
            {
                min = c;
            }
        }
        Console.WriteLine("наиброльшая цифра-" + max + " наименьшая цифра-" + min);
    }
}
